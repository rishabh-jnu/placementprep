class Solution {
public:
    int solveUsingMem(int n, vector<int> &dp){
        if (n == 0 || n == 1) {
            return 1;
        }

        //if ans already exist then return ans
        if(dp[n] != -1){
            return dp[n];
        }
        
        //store and return dp
        dp[n] = solveUsingMem(n-1, dp) + solveUsingMem(n-2, dp);
        return dp[n];
    }

    int climbStairs(int n) {
        //create dp
        vector<int> dp(n + 1, -1);
        int ans = solveUsingMem(n, dp);
        return ans;
        
    }
};
