class Solution {
public:
    int hammingWeight(int n) {
        vector<int>setBit;
        
        while(n>0){
            int bit = n%2;
            setBit.push_back(bit);
            n = n/2;
        }

        long long count =0;
        int i=0;
        while(i<setBit.size()){
            if(setBit[i]==1){
                count++;
            }
            i++;
        }
        return count;
    }
};
