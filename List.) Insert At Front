#include<iostream>
using namespace std;

class Node{
    public:
    int data;
    Node* next;
    Node(int data){
        this->data = data;
        this->next = nullptr;
    }
};

Node* printLL(Node* head){
        Node* temp = head;
        while(temp != nullptr){
        cout<<temp->data<<" ";
        temp = temp->next;
       }
      return temp;
    }

void insertAtHead(Node* &head, int data){
    Node* newNode = new Node(data);
    newNode->next = head;
    head = newNode;
}


int main(){
    Node* head = new Node(20);
    head->next = new Node(30);
    head->next->next = new Node(40);
    
    int data = 10;
    insertAtHead(head, data);
    printLL(head);
    
    return 0;
}
